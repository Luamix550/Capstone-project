{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/RegisterForm.jsx", "src/app/manager/components/AdministrationComponent.jsx", "tailwind.config.ts", "src/app/manager/components/KanbanTable.jsx", "src/app/manager/components/OnlyCardFeed.jsx", "src/app/manager/components/BoxFilter.jsx", "src/app/manager/components/CardUser.jsx", "src/app/manager/components/UserList.jsx"],
  "exclude": ["node_modules"]
}